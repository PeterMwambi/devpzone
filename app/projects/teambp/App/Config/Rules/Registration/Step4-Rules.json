{
    "username": {
        "required": true,
        "min": 5,
        "max": 20,
        "pattern": "/^[A-Za-z0-9]*$/",
        "unique": true,
        "exists": false,
        "method": "ValidateUsername"
    },
    "password": {
        "required": true,
        "min": 6,
        "max": 25,
        "unique": false
    },
    "confirm-password": {
        "required": true,
        "matches": "password"
    },
    "terms-and-conditions": {
        "required": true,
        "values": [
            "accepted"
        ]
    }
}